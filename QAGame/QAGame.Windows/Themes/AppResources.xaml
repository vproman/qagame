<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:QAGame.Themes"
    xmlns:media="using:Windows.UI.Xaml.Media"
    xmlns:system="using:System">
    <SolidColorBrush x:Name="PhoneAccentBrush" Color="DarkGreen"/>
    <SolidColorBrush x:Name="SharedForegroundBrush"
                     Color="{Binding Source={StaticResource TextBoxForegroundThemeBrush}, Path=Color}"
                     Opacity="{Binding Source={StaticResource TextBoxForegroundThemeBrush}, Path=Opacity}"/>    
</ResourceDictionary>
